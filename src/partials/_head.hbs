<head>
	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta http-equiv="Content-Security-Policy" content="default-src 'self'; font-src 'self' https: data:; style-src 'self' 'unsafe-inline' https:; script-src 'self' https:;">
	<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
	<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
	<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
	<link rel="manifest" href="/site.webmanifest" />

	<!--  Essential META Tags -->
	<meta property="title" content="{{ title }}" />
	<meta
		property="og:description"
		content="A unique and exciting space for brilliant and smart-minded women in ethics and compliance to connect, learn and collaborate"
	/>
	<!-- Meta Image -->
	{{#if og_image}}
	<meta property="og:image" content="https://wec-global.org{{og_image}}" />
	{{else}}
	<meta property="og:image" content="https://wec-global.org/wec-global.png" />
	{{/if}}
	<!-- Meta URL -->
	{{#if og_url}}
	<meta property="og:url" content="https://wec-global.org{{og_url}}" />
	{{else}}
	<meta property="og:url" content="https://wec-global.org" />
	{{/if}}

	<!-- Global site tag (gtag.js) - Google Analytics -->
	{{> _gtag }}
	<!-- /Global site tag (gtag.js) - Google Analytics -->

	<!-- Font -->
	<link rel="preconnect" href="https://fonts.gstatic.com" />
	<link
		href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600&display=swap"
		rel="stylesheet"
	/>

	<!-- Cookie Consent by https://www.FreePrivacyPolicy.com -->
	{{> _cookie-policy }}
	<!-- End Cookie Consent -->

	<title>WEC Global | {{ title }}</title>
	<script type="module" src="/src/js/main.js"></script>
</head>